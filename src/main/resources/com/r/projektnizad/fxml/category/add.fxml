<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox alignment="CENTER" prefHeight="890.0" prefWidth="1280.0"  styleClass="container"
      xmlns:fx="http://java.sun.com/JSP/Page"
>
    <VBox alignment="CENTER_LEFT" maxWidth="300" spacing="20.0" VBox.vgrow="ALWAYS">
        <Label fx:id="titleLabel" styleClass="title" text="Dodaj kategoriju"/>
        <VBox>
            <Label text="Ime kategorije"/>
            <TextField fx:id="nameInput"/>
        </VBox>
        <VBox>
            <Label text="Opis kategorije"/>
            <TextField fx:id="descriptionInput"/>
        </VBox>
    </VBox>
</VBox>
